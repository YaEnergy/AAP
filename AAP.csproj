    <Resource Include="Resources\Images\ToolIcons\Light\TextToolIcon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Using Include="System.Windows" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\Images\ProgramIcons\icon2_color.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="Resources\Images\ProgramIcons\Icon_old.ico">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="Resources\Images\ProgramIcons\Icon_old.png">
      <Pack>True</Pack>
